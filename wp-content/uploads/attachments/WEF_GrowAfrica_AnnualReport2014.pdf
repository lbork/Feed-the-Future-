	<script language="javascript"> 
 
			function gId(id)
				{
				return document.getElementById(id);
				}
			function sm(num, show)
				{
						var lnk = gId('lnk' + num);
						if(!lnk) return false;
						if(styleOn) lnk.className = styleOn;
						var objLeft = lnk.parentNode;
						var objTop = gId('divLevelTop'+iblId);
						var ssmenu = gId('divSubmenu' + num);
						if(ssmenu){
							if(show){var initialTop = objTop.offsetHeight;
							if(flagTop) initialTop = -ssmenu.offsetHeight;
							ssmenu.style.left = M_getOffset(objLeft) + 'px';
							ssmenu.style.top  = (initialTop + M_getOffset(objTop, 'top')) + 'px';}
							ssmenu.style.visibility = show ? 'visible':'hidden';var oCont = gId('divConteneur');
							
							if(oCont){
								var decalLeft = 2;
								oCont.style.left = (parseInt(ssmenu.style.left) + decalLeft) + 'px';
								var decalTop  = flagTop ? -2:2;
								oCont.style.top  = (parseInt(ssmenu.style.top) + decalTop) + 'px';
								oCont.style.height = ssmenu.offsetHeight + 'px';
								oCont.style.width  = ssmenu.offsetWidth + 'px';
								oCont.style.display = "block";
								}
							}
						}
						
						
			function hm(){
						for(var i = 1; i<=nbMaxMenu; i++)
								{
									var oDiv = gId('divSubmenu'+i);
									if(oDiv) oDiv.style.visibility = 'hidden';
									var lnk = gId('lnk'+i);
										if(lnk)
											{
												if(styleOff) lnk.className = styleOff;
											}
									}
									var oCont = gId('divConteneur');
									if(oCont)
										{
										oCont.style.display = "none";
										}
							}
							
			function M_getOffset(obj, dir){var px = 0;while(obj.tagName!='BODY'){if(dir && dir=='top') px += obj.offsetTop; else px += obj.offsetLeft;obj = obj.offsetParent;}
			return px;}
	</script> 
			
	<script type="text/javascript"> 
			nbMaxMenu=9;
			
			flagTop=false;
			iblId='64335';
			styleOn='L1 S1625 L1over';
			styleOff='L1 S1625';
	</script> 
	
 
 
 
 
 
 
